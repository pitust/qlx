fn i18n_interpolate { s }
    switch s
        case :i18n_on do return "Enabled!" end
        case :i18n_off do return "Disabled!" end
    end

    printf "i18n failed: {} is not a valid i18n ID!\n" s
    _dbgdump/0
    printflush @message1
    while true do end
end

fn i18n_print { s }
    print i18n_interpolate/1 s

    return null
end